{
  "name": "@kipper/cli",
  "description": "The Kipper programming language Compiler for NodeJS (CLI implementation)",
  "version": "0.1.0",
  "author": "Luna-Klatzer @Luna-Klatzer",
  "bin": {
    "kipper": "./bin/run"
  },
  "dependencies": {
    "@kipper/base": "^0.3.0",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.3",
    "ts-node": "^8.10.2",
    "tslog": "^3.3.0",
    "typescript": "4.5.5"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^8.14.0",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "os-tmpdir": "1.0.2",
    "prettier": "2.4.1",
    "pseudomap": "1.0.2",
    "ts-node": "8.10.2",
    "typescript": "~4.5.5",
    "@types/sinon": "^7.5.2",
    "@sinonjs/fake-timers": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "json-parse-even-better-errors": "2.3.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://wmc-ahif-2021.github.io/Kipper-Web/",
  "bugs": "https://github.com/Luna-Klatzer/Kipper-CLI/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Luna-Klatzer/Kipper-CLI"
  },
  "keywords": [
    "oclif",
    "language",
    "compiler",
    "backend",
    "cli"
  ],
  "license": "GPL-3.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "kipper",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "npm run lint:fix",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "lint": "npm run tslint && npm run prettier",
    "lint:fix": "npm run tslint:fix && npm run prettier",
    "prettier": "prettier --write ./src/",
    "tslint": "eslint src/**/*.ts --ext .ts --config .eslintrc",
    "tslint:fix": "eslint src/**/*.ts --fix --ext .ts --config .eslintrc && npm run prettier",
    "start": "./bin/run"
  },
  "types": "lib/index.d.ts"
}
